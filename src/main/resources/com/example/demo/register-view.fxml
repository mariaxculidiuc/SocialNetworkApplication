        <TextField fx:id="id_password" layoutX="215.0" layoutY="90.0" promptText="Id" />
        <TextField fx:id="firstNameText" layoutX="215.0" layoutY="160.0" promptText="First Name" />
        <TextField fx:id="lastNameText" layoutX="215.0" layoutY="125.0" promptText="Last Name" />
